{
  "name": "@mcp-apps-kit/ui-react-builder",
  "version": "0.2.6",
  "description": "Build tool for React-based MCP application UIs",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite-plugin.d.ts",
        "default": "./dist/vite-plugin.js"
      },
      "require": {
        "types": "./dist/vite-plugin.d.cts",
        "default": "./dist/vite-plugin.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "sdk",
    "react",
    "ui-builder",
    "esbuild"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndurilCode/mcp-apps-kit.git",
    "directory": "packages/ui-react-builder"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.0.0",
    "esbuild": "^0.25.0"
  },
  "peerDependencies": {
    "@mcp-apps-kit/core": "^0.2.0",
    "@mcp-apps-kit/ui-react": "^0.2.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mcp-apps-kit/core": "workspace:*",
    "@mcp-apps-kit/ui-react": "workspace:*",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/types": "^8.51.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
