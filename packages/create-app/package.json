{
  "name": "@mcp-apps-kit/create-app",
  "version": "0.2.0",
  "description": "CLI tool for scaffolding MCP applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "create-mcp-apps-kit": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cli",
    "scaffold",
    "create"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndurilCode/mcp-apps-kit.git",
    "directory": "packages/create-app"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "prompts": "^2.4.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
