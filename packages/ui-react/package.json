{
  "name": "@mcp-apps-kit/ui-react",
  "version": "0.2.3",
  "description": "React bindings for MCP applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "sdk",
    "react",
    "hooks"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndurilCode/mcp-apps-kit.git",
    "directory": "packages/ui-react"
  },
  "dependencies": {
    "@mcp-apps-kit/ui": "^0.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@testing-library/react": "^16.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
